ong(){if(currentSongIndex>0){var idx=currentSongIndex-1;setCurrentSongIndex(idx);}else setCurrentSongIndex(0);}function onPlayerReady(event){setCurrentPlayer(event.target);loadAndPlayCurrentSong(event.target);event.target.playVideo();}// THIS IS OUR EVENT HANDLER FOR WHEN THE YOUTUBE PLAYER'S STATE\n// CHANGES. NOTE THAT playerStatus WILL HAVE A DIFFERENT INTEGER\n// VALUE TO REPRESENT THE TYPE OF STATE CHANGE. A playerStatus\n// VALUE OF 0 MEANS THE SONG PLAYING HAS ENDED.\nfunction onPlayerStateChange(event){var playerStatus=event.data;var player=event.target;if(playerStatus===-1){// VIDEO UNSTARTED\nconsole.log(\"-1 Video unstarted\");}else if(playerStatus===0){// THE VIDEO HAS COMPLETED PLAYING\nconsole.log(\"0 Video ended\");incSong();loadAndPlayCurrentSong(player);//setCurrentSongPlaying(currentList.songs[currentSongIndex]);\n}else if(playerStatus===1){// THE VIDEO IS PLAYED\nconsole.log(\"1 Video played\");}else if(playerStatus===2){// THE VIDEO IS PAUSED\nconsole.log(\"2 Video paused\");}else if(playerStatus===3){// THE VIDEO IS BUFFERING\nconsole.log(\"3 Video buffering\");}else if(playerStatus===5){// THE VIDEO HAS BEEN CUED\nconsole.log(\"5 Video cued\");}}function handleStopPlaying(){currentPlayer.pauseVideo();}function handleStartPlaying(){currentPlayer.playVideo();}function handleNextSong(){incSong();loadAndPlayCurrentSong(currentPlayer);handleStartPlaying();}function handlePrevSong(){decSong();loadAndPlayCurrentSong(currentPlayer);handleStartPlaying();}var currentSongStyle={fontSize:15,textAlign:'center',fontFamily:'Raleway',color:'#463f3a'};var songPlayerButtons={fontSize:35,color:'#463f3a'};var playerButtons=/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{id:\"youtube-player-button\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(Stack[\"a\" /* default */],{direction:\"row\",justifyContent:\"center\",alignItems:\"center\",spacing:0.5,children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(IconButton[\"a\" /* default */],{disabled:!(currentSongIn